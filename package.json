{
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "29.4.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-collections": "^1.3.3"
  },
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": "./dist/*/*.d.ts",
      "default": "./dist/*/*.js"
    }
  },
  "license": "Apache-2.0",
  "name": "razomy.js",
  "packageManager": "npm@10.1.0",
  "private": true,
  "scripts": {
    "init": "npm install",
    "start:dev": "tsc --project tsconfig.lib.json --watch",
    "build": "tsc --project tsconfig.lib.json",
    "test": "jest"
  },
  "type": "module",
  "version": "0.0.0",
  "workspaces": [
    "src/*"
  ]
}
