{
  "compilerOptions": {
    "allowJs": false,
    "declaration": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "dom"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "noUncheckedIndexedAccess": false,
    "noUnusedParameters": false,
    "noImplicitOverride": true,
    "forceConsistentCasingInFileNames": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "verbatimModuleSyntax": true,
    "target": "ESNext",
    "paths": {
      "razomy": [
        "./razomy/index.ts"
      ],
      "@razomy/*": [
        "./razomy/*/index.ts"
      ]
    }
  },
  "exclude": [
    "**/node_modules",
    "**/dist",
    "./razomy/vue",
    "./razomy/nuxt"
  ],
  "references": []
}