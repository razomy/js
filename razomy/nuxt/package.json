  "name": "@razomy/nuxt",
  "peerDependencies": {
    "@nuxt/kit": "^4.3.1",
    "@razomy/schema": "0.0.1-alpha.4",
    "@razomy/schemas": "0.0.1-alpha.4",
    "vue-i18n": "11.2.8",
    "vuetify": "3.11.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "razomy/nuxt",
    "type": "git",
    "url": "git+https://github.com/razomy/js.git"
  },
  "scripts": {
    "build": "nuxt-module-build build && cp -r ./locales ./dist/locales",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "start": "nuxt-module-build build --stub && nuxt-module-build prepare"
  },
  "sideEffects": false,
  "type": "module",
  "version": "0.0.1-alpha.6"
